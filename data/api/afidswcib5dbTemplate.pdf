   quad=($AOI_MIN_LON,$AOI_MAX_LAT,$AOI_MAX_LON,$AOI_MIN_LAT) +
   out="$AFIDSWCIBDB_OUT_IMAGE_NAME" pstring="xxxtst" xvd=n partial=10.0

